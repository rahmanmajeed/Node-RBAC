{
  "compilerOptions": {
    "target": "es2016",
    "experimentalDecorators": true,//important
    "emitDecoratorMetadata": true,//important
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "strictPropertyInitialization": false,//important
    "skipLibCheck": true
  }
}

